{
  "name": "genthetic",
  "version": "0.1.1",
  "description": "Synthetic data generation combining GenAI and good old-fashioned coding.",
  "type": "commonjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup-node",
    "build:clean": "rm -rf dist && tsup-node",
    "test": "pnpm exec tsx --test test/*.test.ts",
    "dev": "pnpm exec tsx"
  },
  "keywords": [
    "genai",
    "ai",
    "synthetic data",
    "fake data",
    "data generation",
    "genkit"
  ],
  "author": "Michael Bleigh",
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.15.5+sha256.8472168c3e1fd0bff287e694b053fccbbf20579a3ff9526b6333beab8df65a8d",
  "dependencies": {
    "@genkit-ai/googleai": "^1.2.0",
    "genkit": "^1.2.0"
  },
  "peerDependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/node": "^22.13.10",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}
